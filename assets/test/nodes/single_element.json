{
  "node_type": "Div",
  "id": "square-element",
  "classes": ["centered-square", "demo-element"],
  "style": {
    "position": "absolute",
    "width": "300px",
    "height": "300px",
    "left": "50%",
    "top": "50%",
    "transform": "translate(-50%, -50%)",
    "background_color": "#3498DB",
    "color": "#FFFFFF",
    "display": "flex",
    "justify_content": "center",
    "align_items": "center",
    "border_style": "solid",
    "border_width": "5px",
    "border_color": "#2980B9",
    "border_radius": "15px",
    "box_shadow": "0 10px 20px rgba(0, 0, 0, 0.3)",
    "text_align": "center",
    "font_size": "24px",
    "font_weight": "bold",
    "letter_spacing": "1px",
    "text_transform": "uppercase",
    "transition": "all 0.3s ease-in-out",
    "z_index": 10,
    "opacity": 0.9,
    "padding": "20px"
  },
  "children": [
    {
      "node_type": "Text",
      "id": "square-text",
      "classes": ["square-content"],
      "style": {
        "color": "#FFFFFF",
        "font_size": "22px",
        "font_weight": "bold",
        "text_shadow": "1px 1px 2px rgba(0, 0, 0, 0.5)"
      },
      "content": "300px Square\nCentered at Origin"
    }
  ]
}